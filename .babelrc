{
  "presets": [
    "react",
    ["env", {
      "targets": { "node": "6.11" },
      "loose": true,
      "modules": "commonjs"
    }]
  ],
  "plugins": [
    "babel-plugin-syntax-dynamic-import",
    "babel-plugin-transform-class-properties",
    "babel-plugin-transform-object-rest-spread",
    ["babel-plugin-css-modules-transform", {"generateScopedName": "[name]__[local]___[hash:base64:5]"}]
  ]
}
